{
  "name": "quick-tab-pick",
  "displayName": "Quick Tab Pick",
  "description": "Quick pick for tabs",
  "publisher": "virasak-com",
  "repository": {
    "type": "git",
    "url": "https://github.com/virasak/quick-tab-pick"
  },
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.97.0"
  },
  "private": true,
  "release": {
    "branches": "main",
    "verifyConditions": [
      "@semantic-release/github"
    ],
    "publish": [
      "@semantic-release/github"
    ],
    "success": [
      "@semantic-release/github"
    ],
    "fail": [
      "@semantic-release/github"
    ]
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quick-tab-pick.action.showTabs",
        "title": "Quick Tab Pick: Show Tabs"
      }
    ]
    "s"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@semantic-release/github": "^11.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3"
  }
}